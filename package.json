{
  "name": "band-evaluation",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "webpack --mode development",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon --watch src/ --exec babel-node -- src/server/server.js",
    "serve": "yarn build && node src/server/index.js",
    "start": "concurrently \"yarn server\" \"yarn client\"",
    "test": "concurrently \"mocha --recursive --require @babel/register --require @babel/polyfill src/test\" \"jest src/client\""
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "node-rdkafka": "^2.5.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.2.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.1.0",
    "css-loader": "^2.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "html-webpack-plugin": "^3.2.0",
    "http-status": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.7",
    "style-loader": "^0.23.1",
    "supertest": "^3.3.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
